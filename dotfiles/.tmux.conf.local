tmux_conf_theme_status_right='#{prefix}  | #{hostname} '

tmux_conf_theme_status_right_fg='brightcyan,brightblack'
tmux_conf_theme_status_right_bg='black,yellow'
tmux_conf_theme_status_right_attr='none,none'

tmux_conf_theme_status_left_fg='brightblack'
tmux_conf_theme_status_left_bg='yellow'

tmux_conf_theme_status_bg='black'

tmux_conf_theme_window_status_current_fg='brightred'
tmux_conf_theme_window_status_current_bg='black'

tmux_conf_theme_window_status_last_fg='brightcyan'
tmux_conf_theme_window_status_last_bg='black'

tmux_conf_theme_window_status_fg='brightgreen'
tmux_conf_theme_window_status_bg='black'

set-option -g history-limit 100000
set-option -g repeat-time 0
